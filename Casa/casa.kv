
#:import Factory kivy.factory.Factory

<Bolao_Janela>:
    HomeWidget:
    DispositivosWidget:
    Celular:


<HomeWidget>:
    ## Fundo Principal
    name: 'homeWidget'
    canvas.before:
        Color:
            rgba: 0.5, 0.5, 0.5, 0.5

        Rectangle:
            size: self.size

    ## Nome da janela
   
    BoxLayout:
        pos_hint:{'center_x':0.5, 'center_y':0.8}
        Label:
            text:'Simulador Casa Automatizada'
            font_size: 50
            color: [1,1,1,1]


    ## Conteudo

        ## Palavras
    Label:
        text:'Hora:'
        font_size: 23
        pos_hint: {'center_x':.2, 'center_y': .55}
        color: [1,1,1,1]
    Label
        text:'Data:'
        font_size: 23
        pos_hint: {'center_x':.2, 'center_y': .45}
        color: [1,1,1,1]
    Label:
        text:'Temperatura:'
        font_size: 23
        pos_hint: {'center_x':.2, 'center_y': .35}
        color: [1,1,1,1]

        ## Caixas

    TextInput:
        text: ""
        id: horaLabel
        font_size: 23
        size_hint: (None, None)
        size: (150, 36)
        pos_hint: {'center_x':.45, 'center_y': .55}

    TextInput:
        text: ''
        id: root.dataLabel
        font_size: 23
        size_hint: (None, None)
        size: (150, 36)
        pos_hint: {'center_x':.45, 'center_y': .45}
    
    TextInput:
        text: ''
        id: root.temperaturaLabel
        font_size: 23
        size_hint: (None, None)
        size: (150, 36)
        pos_hint: {'center_x':.45, 'center_y': .35}

    ## Botões da janela

    BoxLayout:
        pos_hint:{'center_x':1.2, 'center_y':0.56}
        Button:
            text: 'Fechar'
            bold: True
            font_size: 30
            id: sair
            size_hint: (None, None)
            size: (200, 44)
            on_press: app.root.switch_to_homeWidget()
            background_color: 255,0,0 ,1 


    BoxLayout:
        pos_hint:{'center_x':1.2, 'center_y':0.8}
        Button:
            text: 'Começar'
            font_size: 30
            bold: True
            id: inicio
            size_hint: (None, None)
            size: (200, 44)
            background_color: 0, 80, 0, 1
            on_press: app.root.switch_to_dispositivosWidget()


<HiddenIcon_ActionPrevious@ActionPrevious>:
    title: app.title if app.title is not None else 'Gerencia de Missões'
    with_previous: False
    app_icon: ''
    app_icon_width: 0
    app_icon_height: 0
    ## Comment out the following two lines if you wish to have
    ##  ActionGroup(s) and or ActionButtion(s) pushed to the right
    size_hint_x: None
    width: len(self.title) * 15

<ImgButton@Button>:
    size_hint:(None,None)
    size:(530,330)


